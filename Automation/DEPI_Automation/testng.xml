<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Swag Labs Automation Suite">

    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>

        <listener class-name="utils.AllureListener"/>
    </listeners>

    <test name="End to End Tests">
        <classes>
            <class name="tests.LoginTest"/>
            <class name="tests.ProductsTest"/>
            <class name="tests.CartTest"/>
            <class name="tests.CheckoutInfoTest"/>
            <class name="tests.CheckoutOverviewTest"/>
            <class name="tests.CheckoutCompleteTest"/>
        </classes>
    </test>
</suite>